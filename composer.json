{
    "name": "starterteam/starter",
    "type": "typo3-cms-extension",
    "description": "Add description...",
    "license": "proprietary",
    "authors": [
        {
            "name": "Marc Fell",
            "email": "marc@fell.hamburg"
        },
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "typo3/cms-core": "^11.5",
        "ichhabrecht/content-defender": "^3.2"
    },
	"suggest": {
		"starterteam/starter-freya": "Frontend theme based on Twig template engine",
		"starterteam/starter-nessa": "Frontend theme based an TYPO3 Fluid",
		"starterteam/starter-twig": "Processing package for Twig template engine",
		"starterteam/starter-twig-news": "Processing package for EXT:news to work with Twig templates"
	},
    "autoload": {
        "psr-4": {
            "StarterTeam\\Starter\\": "Classes"
        }
    },
	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"allow-plugins": {
			"typo3/class-alias-loader": true,
			"typo3/cms-composer-installers": true
		}
	},
    "extra": {
        "branch-alias": {
            "dev-main": "4.4-dev"
        },
        "typo3/cms": {
            "extension-key": "starter",
			"app-dir": ".Build",
			"web-dir": ".Build/public"
        }
    }
}
